    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Generate random data\n",
    "data = np.random.randn(100)\n",
    "\n",
    "# Plot the data\n",
    "plt.hist(data, bins=10)\n",
    "plt.title('Random Data Histogram')\n",
    "plt.xlabel('Value')\n",
    "plt.ylabel('Frequency')\n",
    "plt.show()"
   ]
  }
